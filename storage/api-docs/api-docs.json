        "/parkings/nearest": {
            "get": {
                "tags": [
                    "Parkings"
                ],
                "summary": "Obtener el parking más cercano a una ubicación",
                "description": "Devuelve el parking más cercano a las coordenadas proporcionadas (latitud y longitud). Si la distancia es mayor a 500 metros, se incluye una advertencia.",
                "operationId": "42f4209d91c87d4fc66e6a06987d91d0",
                "parameters": [
                    {
                        "name": "latitud",
                        "in": "query",
                        "description": "Latitud del punto de búsqueda",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "float",
                            "example": 40.416775
                        }
                    },
                    {
                        "name": "longitud",
                        "in": "query",
                        "description": "Longitud del punto de búsqueda",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "float",
                            "example": -3.70379
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Parking más cercano obtenido correctamente",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Parking más cercano obtenido correctamente"
                                        },
                                        "parking": {
                                            "description": "Datos del parking más cercano",
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "nombre": {
                                                    "type": "string",
                                                    "example": "Parking Central"
                                                },
                                                "latitud": {
                                                    "type": "number",
                                                    "format": "float",
                                                    "example": 40.416775
                                                },
                                                "longitud": {
                                                    "type": "number",
                                                    "format": "float",
                                                    "example": -3.70379
                                                },
                                                "distance": {
                                                    "description": "Distancia en kilómetros",
                                                    "type": "number",
                                                    "format": "float",
                                                    "example": 0.3
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "warning": {
                                            "type": "string",
                                            "example": "Distancia superior a 500 metros, ver registro en la base de datos"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "No se encontraron parkings cercanos",
                        "content": {
                            "application/json": {
